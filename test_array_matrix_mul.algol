// Test matrix multiplication (simplified 2x2 * 2x2)
fn main() -> i64 {
  a: i64[][] = [[1, 2], [3, 4]];
  b: i64[][] = [[5, 6], [7, 8]];

  c: i64[][] = [[0, 0], [0, 0]];

  i: i64 = 0;
  while (i < 2) {
    j: i64 = 0;
    while (j < 2) {
      k: i64 = 0;
      while (k < 2) {
        c[i][j] := c[i][j] + a[i][k] * b[k][j];
        k := k + 1;
      }
      j := j + 1;
    }
    i := i + 1;
  }

  c00: i64 = c[0][0];
  c11: i64 = c[1][1];

  if (c00 == 19 && c11 == 50) {
    return 0;
  } else {
    return 1;
  }
}