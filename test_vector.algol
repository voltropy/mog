// Test vector operations
fn main() -> i64 {
  // Allocate 3-element f32 arrays
  v1: []f32 = new [3]f32;
  v1[0] = 1.0;
  v1[1] = 2.0;
  v1[2] = 3.0;
  
  v2: []f32 = new [3]f32;
  v2[0] = 4.0;
  v2[1] = 5.0;
  v2[2] = 6.0;
  
  // Vector addition (element-wise)
  sum: []f32 = new [3]f32;
  i: i64 = 0;
  while (i < 3) {
    sum[i] = v1[i] + v2[i];
    i = i + 1;
  }
  
  // Print result
  i = 0;
  while (i < 3) {
    print(sum[i]);
    println();
    i = i + 1;
  }
  
  return 0;
}