fn main() -> i64 {
  fd: i64 = open("test_control.txt", O_CREAT | O_WRONLY, 0644);

  if (fd == -1) {
    return 1;
  }

  write(fd, "test\n", 5);

  result: i64 = fcntl(fd, 1);

  if (result == -1) {
    close(fd);
    unlink("test_control.txt");
    return 2;
  }

  result = pathconf(".", 1);

  if (result == -1) {
    close(fd);
    unlink("test_control.txt");
    return 3;
  }

  result = fpathconf(fd, 1);

  if (result == -1) {
    close(fd);
    unlink("test_control.txt");
    return 4;
  }

  close(fd);
  unlink("test_control.txt");

  return 0;
}