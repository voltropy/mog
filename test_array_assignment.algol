// Test array assignment patterns
fn main() -> i64 {
  arr: i64[] = [1, 2, 3, 4, 5];

  i: i64 = 0;
  while (i < 5) {
    arr[i] := arr[i] * arr[i];
    i := i + 1;
  }

  result: i64 = arr[0] + arr[2] + arr[4];

  expected: i64 = 1 + 9 + 25;

  if (result == expected) {
    return 0;
  } else {
    return 1;
  }
}