fn fibonacci(n: i64, a: i64, b: i64) -> i64 {
  if (n == 0) {
    return a;
  }
  if (n == 1) {
    return b;
  }
  next: i64 = a + b;
  return fibonacci(n - 1, b, next);
}

fn fib_helper(a: i64, b: i64, n: i64) -> i64 {
  if (n == 0) {
    return a;
  }
  return fib_helper(b, a + b, n - 1);
}

fn main() -> i64 {
  x: i64 = 10;
  result: i64 = fibonacci(x, 0, 1);
  helper_result: i64 = fib_helper(0, 1, 10);

  return result;
}